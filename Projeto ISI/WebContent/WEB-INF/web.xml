<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
 	<display-name>Projeto ISI</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>Escolher Carro</servlet-name>
		<servlet-class>car.your.controller.EscolherCarro</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Escolher Carro</servlet-name>
		<url-pattern>/carro.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Resultado Carro</servlet-name>
		<servlet-class>car.your.controller.ResultadoCarro</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Resultado Carro</servlet-name>
		<url-pattern>/resultado.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Pegar Categoria</servlet-name>
		<servlet-class>car.your.controller.PegarCategoria</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Pegar Categoria</servlet-name>
		<url-pattern>/categoria.do</url-pattern>
	</servlet-mapping>
</web-app>